{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "https://github.com/drsmile1001/book-walker-helper",
  "description": "Book Walker 助手",
  "default_locale": "zh-TW",
  "permissions": [
    "*://www.bookwalker.com.tw/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "64": "icons/64.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "__MSG_extName__",
    "default_icon": {
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png"
    }
  }
}